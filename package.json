{
    "name": "nodecg-progress",
    "version": "0.0.1",
    "description": "Progress meters for nodecg",
    "authors": [
        "Jason Kendall"
    ],
    "homepage": "",
    "license": "",
    "extension": {
        "path": "extension.js"
    },
    "nodecg": {
        "compatibleRange": "~0.7.0",
        "dashboardPanels": [
          {
            "name": "Progress",
            "title": "Progress",
            "width": 2,
            "headerColor": "#564573",
            "file": "panel.html"
          },{
            "name": "settings",
            "title": "Set Progress Settings",
            "width": 3,
            "file": "dialogs/settings.html",
            "dialog": true,
            "dialogButtons": [
            {
                "name": "Confirm",
                "type": "confirm"
            },
            {
                "name": "Cancel",
                "type": "dismiss"
            }]
          }
        ],
        "graphics": [
        {
            "file": "subs-bar.html",
            "width": 1280,
            "height": 720,
            "singleInstance": false
        },{
            "file": "followers-bar.html",
            "width": 1280,
            "height": 720,
            "singleInstance": false
        },{
            "file": "subs.html",
            "width": 1280,
            "height": 720,
            "singleInstance": false
        },{
            "file": "tips.html",
            "width": 1280,
            "height": 720,
            "singleInstance": false
        },{
            "file": "followers.html",
            "width": 1280,
            "height": 720,
            "singleInstance": false
        }]
    }
}
